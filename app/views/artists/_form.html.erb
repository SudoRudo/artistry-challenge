<div class='error'>
<% if flash[:my_errors]%>
<% flash[:my_errors].each do |error|%>
<p><%= error%></p>
<% end %>
<% end %>
</div>


<%= form_for @artist do |f| %>
<%= f.label :name %><br>
<%= f.text_field :name %><br><br>
<%= f.label :age %><br>
<%= f.number_field :age %><br><br>
<%= f.label :title %><br>
<%= f.text_field :title %><br>
<%= f.submit %>
<%end%>
